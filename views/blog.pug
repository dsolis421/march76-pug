extends layout

block content
  include staticheader.pug
  section.subpage-main-section
    each h in posts
      .blog-block.group
        .headline-block
          a(href=`/blog/${h.quick}`)
            h3= h.headline
          h4= h.desc
          span= h.date
  footer.toggle-nav
    span.icon
      i.fal.fa-angle-up.fa-2x
    a(href="/gallery")
      h3 gallery
    a(href="/about")
      h3 about me
    a(href="/contact")
      h3 contact
    p Copyright &copy 2018 All Rights Reserved
    
